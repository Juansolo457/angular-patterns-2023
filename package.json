{
  "name": "angular-in-app-styleguide",
  "version": "0.0.1",
  "scripts": {
    "analyze:stats": "webpack-bundle-analyzer dist/stats.json",
    "build": "npm run lint && npm run test-build && ng build --configuration=development",
    "build:dynamic-only": "ng build --configuration=dynamic --verbose",
    "build:only": "ng build --configuration=development",
    "build:stats": "ng build --configuration=dynamic --statsJson=true",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "lint": "ng lint",
    "ng": "ng",
    "postinstall": "ngcc --tsconfig './tsconfig.app.json'",
    "refresh-project": "rimraf .cache/* && rimraf coverage/* && rimraf dist/* && rimraf node_modules/* && npm install --legacy-peer-deps --force",
    "start": "ng serve --host=0.0.0.0 --port 4200 --configuration=development",
    "start-dynamic": "ng serve --host=0.0.0.0 --port 4200 --configuration=dynamic",
    "test": "ng test -- --coverage --coverage-reporters=text-summary --coverage-reporters=lcov",
    "test-build": "ng test -- --coverage --coverage-reporters=text-summary --coverage-reporters=cobertura",
    "test-watch": "ng test -- -watch"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1602.0",
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-no-null": "^1.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "jest-zone-patch": "^0.0.10",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^16.0.0",
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "~16.1.8",
    "@angular/compiler-cli": "^16.1.0",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine-core": "~4.6.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "prettier-eslint": "^15.0.1",
    "typescript": "~5.1.3"
  }
}
